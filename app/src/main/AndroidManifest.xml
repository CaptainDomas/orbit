<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alunos.orbit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.Avaliacao" />
        <activity android:name=".View.Cadastro" />
        <activity android:name=".View.PesquisaSaldo" />
        <activity android:name=".View.Login" />
        <activity android:name=".View.ListaOnibus" />
        <activity android:name=".View.Horario" />
        <activity
            android:name=".View.SideMenuActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>